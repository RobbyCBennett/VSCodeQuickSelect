{
	"name": "quick-select",
	"displayName": "Quick Select",
	"description": "Navigate and select words, quotes, and brackets.",
	"version": "1.0.0",
	"publisher": "rob-bennett",
	"repository": {
		"type": "git",
		"url": "https://github.com/RobbyCBennett/VSCodeQuickSelect.git"
	},
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Keymaps",
		"Other"
	],
	"keywords": [
		"Select",
		"Word",
		"Navigate",
		"Quote",
		"Bracket"
	],
	"icon": "icon.png",
	"activationEvents": [
        "onCommand:quickSelect.cursorQuoteSelect",
        "onCommand:quickSelect.cursorQuoteStart",
        "onCommand:quickSelect.cursorQuoteStartSelect",
        "onCommand:quickSelect.cursorQuoteEnd",
        "onCommand:quickSelect.cursorQuoteEndSelect"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "quickSelect.cursorQuoteSelect",
				"title": "Quick Select: Cursor Quote Select",
				"when": "editorTextFocus"
			},
			{
				"command": "quickSelect.cursorQuoteStart",
				"title": "Quick Select: Cursor Quote Start",
				"when": "editorTextFocus"
			},
			{
				"command": "quickSelect.cursorQuoteStartSelect",
				"title": "Quick Select: Cursor Quote Start Select",
				"when": "editorTextFocus"
			},
			{
				"command": "quickSelect.cursorQuoteEnd",
				"title": "Quick Select: Cursor Quote End",
				"when": "editorTextFocus"
			},
			{
				"command": "quickSelect.cursorQuoteEndSelect",
				"title": "Quick Select: Cursor Quote End Select",
				"when": "editorTextFocus"
			}
		]
	}
}
